   <div class=\"button\" @click=\"operation('2')\">2</div>\n      <div class=\"button\" @click=\"operation('3')\">3</div>\n      <div class=\"button l\" @click=\"operation('+')\">+</div>\n    </div>\n    <div class=\"calc-button-row\">\n      <div class=\"button\" @click=\"operation('.')\">.</div>\n      <div class=\"button\" @click=\"operation('0')\">0</div>\n      <div class=\"button\">#</div>\n          <div class=\"button l\" @click=\"equal\" @keyup.enter=\"operation('7')\">=</div>\n      </div>\n    </div>\n  </div>\n\n\n\n</template>\n\n<script>\nexport default{\n    data(){\n        return {\n            input:'',\n            result: 0\n        };\n    },\n    methods:{\n        operation(inp){\n            this.input +=inp ;\n            if(inp!='+'||inp!='-'||inp!='/'||inp!='%')\n            this.equal();\n        },\n        clearAll(){\n            this.input ='';\n            this.result = 0 ;\n        },\n        clear(){\n            this.input = this.input.slice(0,-1) ;\n            this.equal();\n        },\n        equal(){\n            let temp=eval(this.input).toString();\n            // console.log(typeof(temp));\n            if(temp.length>10)\n            this.result =eval(this.input).toExponential() ;\n            else\n            this.result=eval(this.input);\n\n            \n        }\n\n\n    }\n};\n</script>\n\n<style >\n\n* {\n  box-sizing: border-box;\n  font-family: 'Roboto', sans-serif;\n}\n\nbody {\n  background: #d4dada;\n}\n\n.container {\n  width: 50%;\n  margin: auto;\n}\n\n.calc-body {\n  width: 50%;\n  margin: auto;\n  min-height: 400px;\n  border: solid 1px #d4dada;\n  box-shadow: 0 8px 50px -7px black;\n}\n\n.calc-screen {\n  background: #3A4655;\n  width: 180%;\n  height: 150px;\n  padding: 20px;\n}\n\n.calc-operation {\n  text-align: right;\n  color: #727B86;\n  font-size: 21px;\n  padding-bottom: 10px;\n  border-bottom: dotted 1px;\n}\n\n.calc-typed {\n  margin-top: 20px;\n  font-size: 45px;\n  text-align: right;\n  color: #fff;\n}\n\n.calc-button-row {\n  width: 180%;\n  background: #3C4857;\n}\n\n.button {\n  width: 25%;\n  background: #425062;\n  color: #fff;\n  padding: 20px;\n  display: inline-block;\n  font-size: 25px;\n  text-align: center;\n  vertical-align: middle;\n  margin-right: -4px;\n  border-right: solid 2px #3C4857;\n  border-bottom: solid 2px #3C4857;\n \n}\n\n.button.l {\n  color: #2a6dcb;\n  background: #5a9194;\n}\n\n.button.c {\n  color: #D95D4E;\n  background: #425062;\n}\n\n.button:hover {\n  background: #9d4f06;\n}\n\n.button.c:hover,\n.button.l:hover {\n  background: #9d4f06;\n  color: #fff;\n}\n\n\n</style>\n"],"sourceRoot":""}]}